{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Big-Query-To-Azure"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/GoogleBigQuery1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Connector to BigQuery public datasets. ",
				"annotations": [],
				"type": "GoogleBigQuery",
				"typeProperties": {
					"project": "bigquerycopytoadf",
					"requestGoogleDriveScope": false,
					"authenticationType": "UserAuthentication",
					"clientId": "833840357254-31ohjqvvc1fm8t45s4l0kf3gq70ndc80.apps.googleusercontent.com",
					"clientSecret": {
						"type": "SecureString",
						"value": "**********"
					},
					"refreshToken": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}